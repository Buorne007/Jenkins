pipeline{
  agent any
  stages{
    stage('Build'){
      step{
      echo 'Building Job'
      }
    }
    stage('Test'){
      step{
      echo 'Testing Job'
      }
    }
    stage('Deploy'){
      step{
      echo 'Deploying Job'
      }
    } 
    stage('Release'){
      step{
      echo 'Releasing Job'
      }
    }
     stage('Output'){
      step{
      echo 'Application deployed successfully'
      }
    }
  }
}
